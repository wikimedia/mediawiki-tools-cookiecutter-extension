{
	"@metadata": {
		"authors": [
            "WikiTeq Team",
            "{{ cookiecutter.author_name }}"
		]
	},
	{% if cookiecutter.__special_page_name %}
	"{{ cookiecutter.__sp_i18n_prefix }}": "{{ cookiecutter.__special_page_name|capitalize }}",
	"{{ cookiecutter.__sp_i18n_prefix }}-summary": "TODO special page summary",
	"{{ cookiecutter.__sp_i18n_prefix }}-intro": "TODO special page intro",
	{% endif %}
	{% if cookiecutter.__user_right_name %}
	"right-{{ cookiecutter.__user_right_name }}": "TODO user right",
	"action-{{ cookiecutter.__user_right_name }}": "TODO user action",
	{% endif %}
	"{{ cookiecutter.__i18n_prefix }}-desc": "{{ cookiecutter.description }}"
}