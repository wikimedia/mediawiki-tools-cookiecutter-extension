{
	"@metadata": {
		"authors": [
            "WikiTeq Team",
            "{{ cookiecutter.author_name }}"
		]
	},
	{% if cookiecutter.special_page %}
	"{{ cookiecutter.__sp_i18n_prefix }}": "Title of [[Special:{{ cookiecutter.special_page }}]]",
	"{{ cookiecutter.__sp_i18n_prefix }}-summary": "Description at the top of [[Special:{{ cookiecutter.special_page }}]]",
	"{{ cookiecutter.__sp_i18n_prefix }}-intro": "Introduction at the top of [[Special:{{ cookiecutter.special_page }}]]",
	{% endif %}
	{% if cookiecutter.user_right %}
	"right-{{ cookiecutter.user_right }}": "{{ "{{" }}doc-right|{{ cookiecutter.user_right }}{{ "}}" }}",
	"action-{{ cookiecutter.user_right }}": "{{ "{{" }}doc-action|{{ cookiecutter.user_right }}{{ "}}" }}",
	{% endif %}
	"{{ cookiecutter.__i18n_prefix }}-desc": "{{ "{{" }}desc|name={{ cookiecutter.extension_name }}{{ "}}" }}"
}